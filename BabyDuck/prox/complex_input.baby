program complex_input;
var x, y : int; 
    result : float;

void showSum(a : int, b : int) [
    {
        print("The sum is: ", a + b);
    }
];

main {
    x = 5;
    y = 10;
    result = 3.14;

    print("Initial values:");
    print("x = ", x);
    print("y = ", y);
    print("result = ", result);

    while (x < 15) do {
        x = x + 1;
        print("x incremented to: ", x);
    };

    if (x > 10) {
        showSum(x, y);
    } else {
        print("x is too small");
    };
}

end